pkgs:
  xeptoboot:
    version: 0.0.0
    description: A small 2-staged bootloader written in Zig
    license: ISC
    source_url: "https://github.com/leapofazzam123/xeptoboot"
    files:
      LICENSE
      README.md
      build.zig
      lib/*.zig
      stage1/*.zig
      stage1/bios/*.zig
      stage1/uefi/*.zig

deps:
  ominitay/zigvale: ^0.4.0
  leecannon/bitjuggle: ^1.0.1
  leecannon/pc_keyboard: ^0.1.0
  leecannon/x86_64: ^1.0.0
  mattnite/zzz: ^0.0.2
